<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>BlogCreate</display-name>
  <servlet>
    <servlet-name>CreateBlogPostServlet</servlet-name>
    <servlet-class>CreateBlogPostServlet</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>CreateBlogPostServlet</servlet-name>
    <url-pattern>/create</url-pattern>
</servlet-mapping>

<servlet>
    <servlet-name>ViewBlogPostsServlet</servlet-name>
    <servlet-class>ViewBlogPostsServlet</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>ViewBlogPostsServlet</servlet-name>
    <url-pattern>/view</url-pattern>
</servlet-mapping>
  
</web-app>